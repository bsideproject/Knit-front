    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "lodash": "^4.17.20",
    "luxon": "^1.26.0",
    "next": "10.0.6",
    "next-redux-wrapper": "^6.0.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "^7.2.2",
    "react-slick": "^0.28.0",
    "sass": "^1.32.6",
    "slick-carousel": "^1.8.1"
